{
  "$schema": "https://design-tokens.org",
  "color": {
    "primary": {
      "050": { "value": "hsl(var(--primary-050))" },
      "200": { "value": "hsl(var(--primary-200))" },
      "600": { "value": "hsl(var(--primary-600))" },
      "650": { "value": "hsl(var(--primary-650))" },
      "700": { "value": "hsl(var(--primary-700))" }
    },
    "surface": {
      "0": { "value": "hsl(var(--surface-0))" },
      "100": { "value": "hsl(var(--surface-100))" },
      "200": { "value": "hsl(var(--surface-200))" },
      "300": { "value": "hsl(var(--surface-300))" },
      "600": { "value": "hsl(var(--surface-600))" },
      "700": { "value": "hsl(var(--surface-700))" },
      "800": { "value": "hsl(var(--surface-800))" }
    },
    "text": {
      "onPrimary": { "value": "hsl(var(--text-on-primary))" },
      "onSurface": { "value": "hsl(var(--text-on-surface))" }
    },
    "accent": { "600": { "value": "hsl(var(--accent))" } }
  },
  "state": {
    "button": {
      "primary": {
        "default": {
          "fill": { "value": "hsl(var(--button-primary-fill-default))" },
          "border": { "value": "hsl(var(--button-primary-border))" },
          "text": { "value": "hsl(var(--text-on-primary))" }
        },
        "hover": {
          "fill": { "value": "hsl(var(--button-primary-fill-hover))" },
          "border": { "value": "hsl(var(--button-primary-border))" },
          "text": { "value": "hsl(var(--text-on-primary))" }
        },
        "active": {
          "fill": { "value": "hsl(var(--button-primary-fill-active))" },
          "border": { "value": "hsl(var(--button-primary-border))" },
          "text": { "value": "hsl(var(--text-on-primary))" }
        },
        "disabled": {
          "fill": { "value": "hsl(var(--button-primary-fill-disabled))" },
          "border": { "value": "hsl(var(--button-primary-border))" },
          "text": { "value": "hsl(var(--foreground))/40" }
        }
      },
      "secondary": {
        "default": {
          "fill": { "value": "hsl(var(--button-secondary-fill-default))" },
          "border": { "value": "hsl(var(--button-secondary-border-default))" },
          "text": { "value": "hsl(var(--text-on-surface))" }
        },
        "hover": {
          "fill": { "value": "hsl(var(--button-secondary-fill-default))" },
          "border": { "value": "hsl(var(--button-secondary-border-hover))" },
          "text": { "value": "hsl(var(--text-on-surface))" }
        },
        "active": {
          "fill": { "value": "hsl(var(--button-secondary-fill-active))" },
          "border": { "value": "hsl(var(--button-secondary-border-default))" },
          "text": { "value": "hsl(var(--text-on-surface))" }
        },
        "disabled": {
          "fill": { "value": "hsl(var(--button-secondary-fill-default))" },
          "border": { "value": "hsl(var(--button-secondary-border-disabled))" },
          "text": { "value": "hsl(var(--muted-foreground))" }
        }
      }
    },
    "chip": {
      "unselected": {
        "fill": { "value": "transparent" },
        "border": { "value": "hsl(var(--chip-unselected-border))" },
        "text": { "value": "hsl(var(--text-on-surface))" }
      },
      "selected": {
        "fill": { "value": "hsl(var(--chip-selected-fill))" },
        "border": { "value": "hsl(var(--chip-unselected-border))" },
        "text": { "value": "hsl(var(--chip-selected-text))" }
      }
    },
    "tab": {
      "selected": {
        "bar": { "value": "hsl(var(--tab-selected-bar))" }
      },
      "unselected": {
        "border": { "value": "hsl(var(--tab-unselected-border))" }
      }
    }
  },
  "focus": {
    "outline": {
      "color": { "value": "hsl(var(--focus-outline-color))" },
      "width": { "value": "var(--focus-outline-width)" },
      "offset": { "value": "var(--focus-outline-offset)" }
    }
  }
}
